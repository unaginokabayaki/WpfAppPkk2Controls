<Window x:Class="WpfAppPkk2Controls.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppPkk2Controls"
        mc:Ignorable="d"
        Title="Window3" Height="450" Width="800">
    <Grid>
        <Canvas>
            <Rectangle Canvas.Left="20" Canvas.Top="20" Width="100" Height="100" Fill="SlateGray" Panel.ZIndex="1"/>
            <Ellipse Canvas.Left="40" Canvas.Top="40" Width="100" Height="100" Fill="Salmon" Panel.ZIndex="0"/>
            <Line X1="10" Y1="10" X2="200" Y2="30" Stroke="Blue" StrokeThickness="2"/>
            <Button Canvas.Left="80" Canvas.Top="80" Content="Button" Height="30" Width="100" Panel.ZIndex="2"/>
        </Canvas>
    </Grid>
</Window>
